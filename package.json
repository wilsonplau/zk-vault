{
  "name": "zk-vault",
  "version": "1.0.0",
  "scripts": {
    "start": "hardhat node",
    "test:v1": "hardhat test \"test/VaultV1.spec.ts\"",
    "test:v2": "hardhat test \"test/VaultV2.spec.ts\"",
    "test:v3": "hardhat test \"test/VaultV3.spec.ts\"",
    "generate": "hardhat run --network localhost scripts/generatePoseidonHash.ts",
    "hack:v1": "hardhat run --network localhost scripts/hackVaultV1.ts",
